= content_for :head do
  %meta{property: 'og:image', content: ticker_url(@pair, format: :png), data: {reactid: 10}}

%h1= @pair

%h3 Asks

%ul
  - @ticker["asks"].each do |ask|
    %li
      = ask.first
      = ask.last
    
%hr

%h3 Bids

%ul
  - @ticker["bids"].each do |bid|
    %li
      = bid.first
      = bid.last

= image_tag ticker_url(@pair, format: :png)

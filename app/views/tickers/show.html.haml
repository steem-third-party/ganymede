= content_for :head do
  %meta{property: 'og:image', content: ticker_url(@pair, md5: md5_title, format: :png), data: {reactid: 10}}
  %meta{property: 'og:image:width', content: 1200}

%h1= @pair

%h3 Asks

%ul
  - @ticker["asks"].each do |ask|
    %li
      = ask.first
      = ask.last
    
%hr

%h3 Bids

%ul
  - @ticker["bids"].each do |bid|
    %li
      = bid.first
      = bid.last

= adaptive_media_single_photo(ticker_url(@pair, md5: md5_title, format: :png))